      elsif($rbdnum == 11) {
        $b_drv11->configure(-background => 'yellow');
      }
      if($playback) { last; }
    }
    elsif(/^\*\*\*Rb:/) {
      $trobot0->insert('end', $_);
      if($rbdnum == 0) {
        $b_drv0->configure(-background=>'grey', -text=>$slottapes[$snum-1]);
